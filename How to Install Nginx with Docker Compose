sudo apt update -y
sudo apt-get install apt-transport-https software-properties-common ca-certificates curl gnupg lsb-release -y
sudo curl -fsSL https://download.docker.com/linux/ubuntu/gpg | apt-key add -
sudo add-apt-repository "deb [arch=amd64] https://download.docker.com/linux/ubuntu focal stable"
sudo apt-get update -y
sudo apt-get install docker-ce docker-ce-cli -y
sudo docker version
curl -s https://api.github.com/repos/docker/compose/releases/latest | grep browser_download_url | grep docker-compose-linux-x86_64 | cut -d '"' -f 4 | wget -qi -
#chmod +x docker-compose-Linux-x86_64
sudo chmod +x docker-compose-Linux-x86_64
sudo mv docker-compose-Linux-x86_64 /usr/bin/docker-compose
sudo docker-compose --version
sudo mkdir -p project/src
sudo nano project/src/index.html

sudo nano project/docker-compose.yaml


sudo cd project
sudo docker-compose up -d








